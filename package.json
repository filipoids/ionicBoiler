{
  "devDependencies": {
    "del": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-concat": "^2.5.2",
    "gulp-filter": "^2.0.2",
    "gulp-if-else": "^1.0.3",
    "gulp-imagemin": "^2.2.1",
    "gulp-livereload": "^3.8.0",
    "gulp-minify-html": "^1.0.1",
    "gulp-ruby-sass": "^1.0.0-alpha.3",
    "gulp-uglify": "^1.1.0",
    "gulp-watch": "^4.1.1",
    "minimist": "^1.1.1",
    "require-dir": "^0.1.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "configs": {
    "directories": {
      "archive": "archive",
      "src": {
        "root": "src",
        "js": "src/js",
        "vendor": "src/js/vendor",
        "isobar": "src/js/isobar",
        "css": "src/css",
        "images": "src/img",
        "fonts": "src/css/__assets/fonts",
        "assets": "src/css/__assets/images",
        "extra": []
      },
      "public": {
        "root": "www",
        "js": "www/js",
        "vendor": "www/js/vendor",
        "css": "www/css",
        "images": "www/img",
        "fonts": "www/css/assets/fonts",
        "assets": "www/css/assets/images",
        "extra": []
      },
      "test": "test"
    }
  },
  "homepage": "boiler",
  "license": {
    "type": "MIT",
    "url": "https://github.com/juliosgarbi/boiler"
  },
  "name": "boiler",
  "private": true,
  "scripts": {
    "build": "gulp build",
    "test": "gulp archive && mocha --reporter spec --timeout 5000"
  },
  "version": "0.1.0",
  "dependencies": {
    "ionic": "^1.7.12"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "ios"
  ]
}
